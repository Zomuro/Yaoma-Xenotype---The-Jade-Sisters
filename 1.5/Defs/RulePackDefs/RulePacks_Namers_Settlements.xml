<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>YaomaCore_NamerSettlement</defName>
    <include>
      <li>YaomaCore_WordUtility</li>
    </include>
    <rulePack>
      <rulesStrings>	
		<li>r_name->[YaomaFamily] [YaomaGroup]</li> <!-- i.e. Cao Covent -->
		<li>r_name->[YaomaGiven] [YaomaGroup]</li> <!-- i.e. Aiying Clan -->
		
		<li>r_name->[YaomaGroup] of [YaomaElements]</li> <!-- i.e. Sect of Space -->
		<li>r_name->[YaomaGroup] of the [YaomaMyth] </li> <!-- i.e. Sect of the Taotie -->		
        <li>r_name->[YaomaGroup] of the [YaomaMyth]'s [Positive] </li> <!-- i.e. Sect of the Taotie's Blessing -->
        
		<li>r_name->[YaomaElements]-[YaomaElementsAction] [YaomaGroup]</li> <!-- i.e. Space-Rending Sect -->
		<li>r_name->[YaomaElements] [YaomaItem] [YaomaGroup]</li>	<!-- i.e. Space Blade Sect -->
		<li>r_name->[YaomaElements] [YaomaMyth] [YaomaGroup]</li> <!-- i.e. Space Immortal Sect  -->

		<li>r_name->[YaomaMyth] [YaomaItem] [YaomaGroup]</li> <!-- i.e. Tiger Blade Sect -->
		<li>r_name->[YaomaMyth] [YaomaElements] [YaomaGroup]</li> <!-- i.e. Tiger Space Sect -->	

		<li>r_name->[YaomaItem] [YaomaMyth] [YaomaGroup]</li> <!-- i.e. Blade Tiger Sect -->			
 
		<li>r_name->[YaomaAdj] [YaomaItem] [YaomaGroup]</li>  <!-- i.e. Dominating Blade Sect -->	
		<li>r_name->[YaomaAdj] [YaomaMyth] [YaomaGroup]</li> <!-- i.e. Dominating Immortal Sect -->	
		<li>r_name->[YaomaAdj] [YaomaElements] [YaomaGroup]</li> <!-- i.e. Dominating Rain Sect -->	
      </rulesStrings>
    </rulePack>
  </RulePackDef>

</Defs>
